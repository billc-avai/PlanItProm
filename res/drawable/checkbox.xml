<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_window_focused="false" android:drawable="@drawable/check_on"
          android:state_enabled="true" android:state_checked="true"></item>
    <item android:state_window_focused="false" android:drawable="@drawable/check_off"
          android:state_enabled="true" android:state_checked="false"></item>
    <item android:drawable="@drawable/check_on_pressed" android:state_enabled="true"
          android:state_pressed="true" android:state_checked="true"></item>
    <item android:drawable="@drawable/check_off_pressed" android:state_enabled="true"
          android:state_pressed="true" android:state_checked="false"></item>
    <item android:state_focused="true" android:drawable="@drawable/check_on_focused"
          android:state_enabled="true" android:state_checked="true"></item>
    <item android:state_focused="true" android:drawable="@drawable/check_off_focused"
          android:state_enabled="true" android:state_checked="false"></item>
    <item android:drawable="@drawable/check_off" android:state_enabled="true"
          android:state_checked="false"></item>
    <item android:drawable="@drawable/check_on" android:state_enabled="true"
          android:state_checked="true"></item>
    <item android:state_window_focused="false" android:drawable="@drawable/check_on_disabled"
          android:state_checked="true"></item>
    <item android:state_window_focused="false" android:drawable="@drawable/check_off_disabled"
          android:state_checked="false"></item>
    <item android:state_focused="true" android:drawable="@drawable/check_on_disabled_focused"
          android:state_checked="true"></item>
    <item android:state_focused="true" android:drawable="@drawable/check_off_disabled_focused"
          android:state_checked="false"></item>
    <item android:drawable="@drawable/check_off_disabled" android:state_checked="false"></item>
    <item android:drawable="@drawable/check_on_disabled" android:state_checked="true"></item>
</selector>